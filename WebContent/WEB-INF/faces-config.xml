<?xml version="1.0" encoding="utf-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
   
    	
   <application>  
        <variable-resolver>  
            org.springframework.web.jsf.DelegatingVariableResolver   
        </variable-resolver>  
    </application> 
   
	<managed-bean>
		<managed-bean-name>referenceBean</managed-bean-name>
		<managed-bean-class>org.earthChem.presentation.jsf.ReferenceBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property> 
		 <property-name>referenceManager</property-name>  
	          <value>#{referenceManager}</value>  
           </managed-property>  
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>logoutBean</managed-bean-name>
		<managed-bean-class>org.earthChem.presentation.jsf.LogoutBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

</faces-config>